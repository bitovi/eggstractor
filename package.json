{
  "name": "eggstractor",
  "version": "1.0.0",
  "description": "Eggstractor",
  "main": "code.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "npm run generate-theme && webpack --mode=development --watch",
    "visualizer": "lite-server --baseDir=./visualizer",
    "prettier": "prettier --write .",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "watch": "npm run build -- --watch",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "generate-theme": "node scripts/generate-theme.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,mdx,yml,yaml,css,scss,less,html}": [
      "prettier --write"
    ]
  },
  "author": "Bitovi",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@figma/eslint-plugin-figma-plugins": "^0.16.1",
    "@figma/plugin-typings": "^1.117.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "css-loader": "^7.1.2",
    "esbuild": "^0.19.0",
    "eslint": "^8.57.1",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lint-staged": "^16.1.6",
    "lite-server": "^2.6.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "sass-rem": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "culori": "^4.0.1",
    "highlight.js": "^11.11.1",
    "lodash.merge": "^4.6.2"
  }
}
