{
  "name": "@eggstractor/source",
  "description": "Eggstractor Figma Plugin",
  "version": "0.0.0",
  "author": "Bitovi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nx run-many -t build --projects=@eggstractor/ui,@eggstractor/figma --parallel --output-style=stream",
    "dev": "nx run-many -t build:watch --projects=@eggstractor/ui,@eggstractor/figma --parallel --output-style=stream",
    "start": "npm run dev",
    "test": "nx run-many -t test --parallel --output-style=stream",
    "lint": "nx run-many -t lint:fix --parallel --output-style=stream",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mts,cts}": [
      "nx affected -t lint --files",
      "node tools/format-staged.mjs"
    ],
    "*.{json,md,css,scss,html,yml,yaml}": [
      "node tools/format-staged.mjs"
    ]
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.117.0",
    "@figma/eslint-plugin-figma-plugins": "github:figma/eslint-plugin-figma-plugins#532a880",
    "@nx/devkit": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/playwright": "21.5.2",
    "@nx/react": "21.5.2",
    "@nx/vite": "21.5.2",
    "@nx/web": "21.5.2",
    "@nx/workspace": "21.5.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/node": "20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lint-staged": "^16.2.1",
    "nx": "21.5.2",
    "prettier": "^3.6.2",
    "sass": "^1.55.0",
    "tslib": "^2.3.0",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
